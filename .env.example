# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Project URL (from your Supabase dashboard)
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous/Public Key (from your Supabase dashboard)
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# These are used for API routes (already configured in Vercel)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_JWT_SECRET=your-jwt-secret-here

# Frontend API URL (for local development)
REACT_APP_API_URL=http://localhost:3000